#!/bin/zsh
# acugen: AtCoder URL Generate
USAGE="acugen <contest_ids> <task_ids>"

if [ $# -lt 2 ]; then
    echo $USAGE
    exit 1
fi

CONTEST_IDS=($(eval "echo $1"))
TASK_IDS=($(eval "echo $2"))

for c in ${CONTEST_IDS[*]}; do
    for t in ${TASK_IDS[*]}; do
        echo https://atcoder.jp/contests/${c}/tasks/${c}_${t}
    done
done
